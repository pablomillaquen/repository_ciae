<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/repositoryciae/db" VERSION="20211015" COMMENT="XMLDB file for Moodle local/repositoryciae"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_repositoryciae_files" COMMENT="Repository files for CIAE Traditional Educators">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="250" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="grades" TYPE="char" LENGTH="90" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="territory" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="materialtype" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="culturalcontent" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="link" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="filetype" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="image" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="oa" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="abstract" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="axis" TYPE="char"  LENGTH="90" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>